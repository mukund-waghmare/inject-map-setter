<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">



	<bean id="shop" class="com.ty.Shop">
	<property name="item" value="fruites"></property>
	<property name="name" value="abc"></property>
	 
	<property name="map">
	<map>
	<entry key="Apple" value="89.23"></entry>
	
	<entry key="Grepes" value="89.23"></entry>
	
	<entry key="Papaya" value="89.23"></entry>
	
	<entry key="Persimos" value="89.23"></entry>
	
	<entry key="dragonFruite" value="89.23"></entry>
	</map>
	</property>
	</bean>
    

</beans>